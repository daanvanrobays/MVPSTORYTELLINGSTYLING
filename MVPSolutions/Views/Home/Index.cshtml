
@{
    ViewBag.Title = "Marie Van Puyenbroeck";
}
<!-- Header -->
<header id="top" class="header">
    <div class="text-vertical-top text-main">
        <img src="~/images/logotrans.png" />
        <h1 class="header-text">Storytelling Styling</h1>
        <h3 class="header-smaller-text">MARIE VAN PUYENBROECK</h3>
        <br>
        <a href="#portfolio" class="btn btn-dark btn-lg">Click Me!</a>
    </div>
</header>
<!-- Portfolio -->
<section id="portfolio" class="portfolio">
    <div class="container">
        <div class="row">
            <div class="col-lg-10 col-lg-offset-1 text-center text-main">
                <h2>Stories</h2>
                <p class="mobileShow pink">(Tap to see stories)</p>
                <hr class="small">
                <div class="row">
                    <!-- Queen Louise Story -->
                    <div class="col-xs-12 col-sm-12 col-md-offset-3 col-md-6 col-lg-offset-3 col-lg-6">
                        <div class="portfolio-item view view-tenth" onclick="">
                            <img src="~/images/queenluisa/Queen Luisa-banner-small.jpg" class="img-responsive" />
                            <div class="mask"></div>
                            <div class="content">
                                <h2>Queen Luisa</h2>
                                <p></p>
                                <a href="@Url.Action("QueenLuisa","Stories")" class="info">See Story</a>
                            </div>
                        </div>
                    </div>
                    <!-- Moodswings Story -->
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                        <div class="portfolio-item view view-tenth" onclick="">
                            <img src="/images/TestIMG_0421.JPG" class="img-responsive" />
                            <div class="mask"></div>
                            <div class="content">
                                <h2>Moodswings</h2>
                                <p></p>
                                <a href="@Url.Action("Moodswings","Stories")" class="info">See story</a>
                            </div>
                        </div>
                    </div>
                    <!-- 22.3 en Rose Story -->
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                        <div class="portfolio-item view view-tenth" onclick="">
                            <img src="/images/Mini (20 van 21).jpg" class="img-responsive" />
                            <div class="mask"></div>
